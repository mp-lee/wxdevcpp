<!-- **************    Mingw compiler  **************   -->

        <compiler type="MingW" name="Mingw gcc">
                <default_compiler>true</default_compiler>
		<programs>
			<c_compiler>gcc.exe</c_compiler>
			<cpp_compiler>g++.exe</cpp_compiler>
			<make>make.exe</make>
			<windres>windres.exe</windres>
			<dllwrap>g++.exe</dllwrap>
			<debugger>gdb.exe</debugger>
			<gprof>gprof.exe</gprof>
		</programs>
		<formatting>
			<comp>-c </comp>
			<link>-o </link>
			<lib>-l </lib>
			<libdir>-L </libdir>
			<include>-I </include>
			<output>-o </output>
			<rcinclude>--input-format=rc -i </rcinclude>
			<rcoutput>-O coff -o </rcoutput>
			<includedir>--include-dir </includedir>
			<checksyntaxcompiler>-S </checksyntaxcompiler>
			<checksyntaxoutput>-o nul </checksyntaxoutput>
		</formatting>
		<captions>
			<dllwrap>link : </dllwrap>
			<windres>rc : </windres>
		</captions>
		<binaries>
			<binary type="0">make</binary>
			<binary type="1">cl</binary>
			<binary type="2">cl</binary>
			<binary type="3">link</binary>
			<binary type="4">rc</binary>
		</binaries>
		<switches>
			<category name="C Compiler">

  				<switch name="Support all ANSI standard C programs (-ansi)" command="-ansi">
   				 <enabled>false</enabled>
   				 <fieldtype type="boolean"></fieldtype>
   				 <switchtype>c,c++</switchtype>
   				 <excludefrom></excludefrom>
				 <selection>false</selection>
				 <default>false</default>
				 <enabled>true</enabled>
   				</switch>

 				<switch name="Attempt to support some aspects of traditional C preprocessors (-traditional-cpp)" command="-traditional-cpp">
    				  <enabled>false</enabled>
    				  <fieldtype type="boolean"></fieldtype>
    				  <switchtype>c,c++</switchtype>
    				  <excludefrom></excludefrom>
				 <selection>false</selection>
				 <default>false</default>
				 <enabled>true</enabled>
    				</switch>

  				<switch name="Inhibit all warning messages (-w)" command="-w">
    				  <enabled>false</enabled>
    				  <fieldtype type="boolean"></fieldtype>
    				  <switchtype>c,c++</switchtype>
    				  <excludefrom></excludefrom>
				 <selection>false</selection>
				 <default>false</default>
				 <enabled>true</enabled>
    				</switch>

			</category>

			<category name="C++ Compiler">

  				<switch name="Turn off all access checking (-fno-access-control)" command="-fno-access-control">
    				  <enabled>false</enabled>
    				  <fieldtype type="boolean"></fieldtype>
				  <switchtype>c,c++</switchtype>
    				  <excludefrom></excludefrom>
				 <selection>false</selection>
				 <default>false</default>
				 <enabled>true</enabled>
    				</switch>

  				<switch name="Accept $ in identifiers (-fdollar-in-identifiers)" command="-fdollar-in-identifiers">
    				  <enabled>false</enabled>
    				  <fieldtype type="boolean"></fieldtype>
    				  <switchtype>c,c++</switchtype>
    				  <excludefrom></excludefrom>
				 <selection>false</selection>
				 <default>false</default>
				 <enabled>true</enabled>
    				</switch>

  				<switch name="Placeholder for obsolete switch (-fmemoized)" command="">
    				  <enabled>false</enabled>
    				  <fieldtype type="boolean"></fieldtype>
    				  <switchtype>c,c++</switchtype>
    				  <excludefrom></excludefrom>
				 <selection>false</selection>
				 <default>false</default>
				 <enabled>true</enabled>
    				</switch>

			</category>

			<category name="Code Generation">

  				<switch name="Enable exception handling (-fexceptions)" command="-fexceptions">
    				  <enabled>false</enabled>
    				  <fieldtype type="boolean"></fieldtype>
    				  <switchtype>c,c++</switchtype>
    				  <excludefrom></excludefrom>
				 <selection>false</selection>
				 <default>false</default>
				 <enabled>true</enabled>
    				 </switch>

  				<switch name="Use same size for double and float (-fshort-double)" command="-fshort-double">
    				  <enabled>false</enabled>
    				  <fieldtype type="boolean"></fieldtype>
    				  <switchtype>c,c++</switchtype>
    				  <excludefrom></excludefrom>
				 <selection>false</selection>
				 <default>false</default>
				 <enabled>true</enabled>
    				</switch>

  				<switch name="Put extra commentary information in the generated assembly code (-fverbose-asm)" command="-fverbose-asm">
    				  <enabled>false</enabled>
    				  <fieldtype type="boolean"></fieldtype>
    				  <switchtype>c,c++</switchtype>
    				  <excludefrom></excludefrom>
				 <selection>false</selection>
				 <default>false</default>
				 <enabled>true</enabled>
    				</switch>

				<switch name="Generate instructions for the following machine (-march)" command="-march=">
     				  <enabled>true</enabled>
     				  <fieldtype type="enumerated">
				     <option value="">None</option>
                                     <option value="i386">i386</option>
                                     <option value="i486">i486</option>
                                     <option value="i586">i586</option>
                                     <option value="i686">i686</option>
                                     <option value="pentium">Pentium</option>
                                     <option value="pentium-mmx">Pentium MMX</option>
                                     <option value="pentiumpro">Pentium Pro</option>
                                     <option value="pentium2">Pentium 2</option>
                                     <option value="pentium3">Pentium 3</option>
                                     <option value="pentium4">Pentium 4</option>
                                     <option value="k6">K6</option>
                                     <option value="k6-2">K6-2</option>
                                     <option value="k6-3">K6-3</option>
                                     <option value="athlon">Athlon</option>
                                     <option value="athlon-tbird">Athlon TBird</option>
                                     <option value="athlon-4">Athlon-4</option>
                                     <option value="athlon-xp">Athlon XP</option>
                                     <option value="athlon-xp">Athlon XP</option>
                                     <option value="winchip-c6">Winchip C6</option>
                                     <option value="winchip2">Winchip 2</option>
                                     <option value="k8">K8</option>
                                     <option value="c3">C3</option>
                                     <option value="c3-2">C3-2</option>
				  </fieldtype>
     				  <switchtype>c,c++,linker</switchtype>
     				  <excludefrom></excludefrom>
     				  <selection>0</selection>
     				  <default>0</default>
				 </switch>

				<switch name="Enable use of processor specific built-in functions (-m)" command="-m">
     				  <fieldtype type="enumerated">
                                     <option value="">None</option>
                                     <option value="mmx">MMX</option>
                                     <option value="sse">SSE</option>
                                     <option value="sse2">SSE 2</option>
                                     <option value="pni">PNI</option>
                                     <option value="3dnow">3D Now</option>
				  </fieldtype>
     				  <switchtype>c,c++,linker</switchtype>
     				  <excludefrom></excludefrom>
     				  <selection>0</selection>
     				  <default>0</default>
				  <enabled>true</enabled>
  				</switch>

			</category>

			<category name="Optimization">

 				<switch name="Perform a number of minor optimizations (-fexpensive-optimizations)" command="-fexpensive-optimizations">
    				  <fieldtype type="boolean"></fieldtype>
    				  <switchtype>c,c++</switchtype>
    				  <excludefrom></excludefrom>
				 <selection>false</selection>
				 <default>false</default>
				 <enabled>true</enabled>
    				</switch>

			</category>

			<category name="Optimization/Further Optimization">

  				<switch name="Optimize (-O1)" command="-O1">
    				  <isgroup>true</isgroup>
    				  <fieldtype type="boolean"></fieldtype>
    				  <switchtype>c,c++</switchtype>
    				  <excludefrom></excludefrom>
				 <selection>false</selection>
				 <default>false</default>
				 <enabled>true</enabled>
    				 </switch>

  				<switch name="Optimize more (-O2)" command="-O2">
    				  <isgroup>true</isgroup>
    				  <fieldtype type="boolean"></fieldtype>
    				  <switchtype>c,c++</switchtype>
    				  <excludefrom></excludefrom>
				 <selection>false</selection>
				 <default>false</default>
				 <enabled>true</enabled>
    				</switch>

  				<switch name="Optimize best (-O3)" command="-O3">
    				  <isgroup>true</isgroup>
    				  <fieldtype type="boolean"></fieldtype>
    				  <switchtype>c,c++</switchtype>
    				  <excludefrom></excludefrom>
				 <selection>false</selection>
				 <default>false</default>
				 <enabled>true</enabled>
    				</switch>

			</category>

			<category name="Code profiling">

 				<switch name="Generate profiling info for analysis (-pg)" command="-pg">
    				  <fieldtype type="boolean"></fieldtype>
    				  <switchtype>c,c++</switchtype>
    				  <excludefrom></excludefrom>
				 <selection>false</selection>
				 <default>false</default>
				 <enabled>true</enabled>
    				</switch>

			</category>

			<category name="Linker">

 				<switch name="Link an Objective C program (-lobjc)" command="-lobjc">
    				  <fieldtype type="boolean"></fieldtype>
    				  <switchtype>linker</switchtype>
    				  <excludefrom></excludefrom>
				 <selection>false</selection>
				 <default>false</default>
				 <enabled>true</enabled>
    				</switch>

  				<switch name="Generate debugging information (-g3)" command="-g3">
    				  <fieldtype type="boolean"></fieldtype>
    				  <switchtype>c,c++,linker</switchtype>
    				  <excludefrom></excludefrom>
				 <selection>false</selection>
				 <default>false</default>
				 <enabled>true</enabled>
    				</switch>

  				<switch name="Do not use standard system startup files or libraries (-nostdlib)" command="-nostdlib">
    				  <fieldtype type="boolean"></fieldtype>
    				  <switchtype>c,c++,linker</switchtype>
    				  <excludefrom></excludefrom>
				 <selection>false</selection>
				 <default>false</default>
				 <enabled>true</enabled>
    				</switch>

  				<switch name="Do not create a console window (-mwindows)" command="-mwindows">
    				  <fieldtype type="boolean"></fieldtype>
    				  <switchtype>c,c++,linker</switchtype>
    				  <excludefrom></excludefrom>
				 <selection>true</selection>
				 <default>true</default>
				 <enabled>true</enabled>
    				</switch>

 				<switch name="Strip executable (-s)" command="-s">
    				 <fieldtype type="boolean"></fieldtype>
    				  <switchtype>c,c++,linker</switchtype>
    				  <excludefrom></excludefrom>
				 <selection>false</selection>
				 <default>false</default>
				 <enabled>true</enabled>
  				</switch>

			</category>
		</switches>
       </compiler>
       
<!-- **************    MS VC++ compiler  **************   -->

	<compiler type="Visual C++" name="Microsoft Visual C++">
                <default_compiler>false</default_compiler>
		<programs>
			<c_compiler>cl.exe</c_compiler>
			<cpp_compiler>cl.exe</cpp_compiler>
