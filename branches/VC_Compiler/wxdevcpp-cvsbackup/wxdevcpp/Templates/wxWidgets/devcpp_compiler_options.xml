			<windres>rc.exe</windres>
			<dllwrap>link.exe</dllwrap>
			<debugger></debugger>
			<gprof></gprof>
		</programs>
		<formatting>
			<comp>/nologo /c</comp>
			<link>/nologo /OUT:</link>
			<lib></lib>
			<libdir>/LIBPATH:</libdir>
			<include>/I</include>
			<output>/Fo</output>
			<rcinclude>/r /i</rcinclude>
			<rcoutput>/fo</rcoutput>
			<includedir>/I </includedir>
			<checksyntaxcompilerlabel>/nologo /c</checksyntaxcompilerlabel>
			<checksyntaxoutputlabel>/OUT:nul</checksyntaxoutputlabel>
		</formatting>
		<captions>
			<dllwrap></dllwrap>
			<windres></windres>
		</captions>
		<binaries>
			<binary type="0">make</binary>
			<binary type="1">cl</binary>
			<binary type="2">cl</binary>
			<binary type="3">link</binary>
			<binary type="4">rc</binary>
		</binaries>
		<switches>
			<category name="Optimizations">
				<switch name="Optimize for" command="">
